<?xml version="1.0" encoding="utf-8"?>

    <androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/cL"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF"
        tools:context=".ui.insetos.ResultadoFragment">


    <TextView
        android:id="@+id/outline4"
        android:layout_width="144dp"
        android:layout_height="168dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/outline_green"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/outline3"
        android:layout_width="298dp"
        android:layout_height="108dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/outline_green"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivReferencia" />

    <TextView
        android:id="@+id/description"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="2dp"
        android:background="#FFFFFF"
        android:text="@string/descri_o"
        android:textColor="#035815"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/tvDescription"
        app:layout_constraintEnd_toEndOf="@+id/outline3"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="@+id/outline3"
        app:layout_constraintTop_toBottomOf="@+id/ivReferencia"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/outline2"
        android:layout_width="184dp"
        android:layout_height="74dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/outline_green"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ivReferencia"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/ivReferencia"
        android:layout_width="135dp"
        android:layout_height="150dp"
        android:src="@drawable/ic_camera"
        app:layout_constraintBottom_toBottomOf="@+id/outline4"
        app:layout_constraintEnd_toEndOf="@+id/outline4"
        app:layout_constraintHorizontal_bias="0.421"
        app:layout_constraintStart_toStartOf="@+id/outline4"
        app:layout_constraintTop_toTopOf="@+id/outline4"
        app:layout_constraintVertical_bias="0.444" />

    <TextView
        android:id="@+id/inseto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginBottom="4dp"
        android:background="#FFFFFF"
        android:text="@string/insetoDetalhe"
        android:textColor="#035815"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/tvOrdem"
        app:layout_constraintEnd_toEndOf="@+id/outline2"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="@+id/outline2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/outline"
        android:layout_width="184dp"
        android:layout_height="74dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/outline_green"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ivReferencia"
        app:layout_constraintTop_toBottomOf="@+id/outline2" />

    <TextView
        android:id="@+id/tvQuestoes"
        android:layout_width="169dp"
        android:layout_height="58dp"
        android:scrollbarThumbVertical="@android:color/holo_green_dark"
        android:scrollbars="vertical"
        android:text="@string/tvcaracteristicas"
        android:textAlignment="center"
        android:textColor="#000000"
        android:verticalScrollbarPosition="right"
        app:layout_constraintBottom_toBottomOf="@+id/outline"
        app:layout_constraintEnd_toEndOf="@+id/outline"
        app:layout_constraintStart_toStartOf="@+id/outline"
        app:layout_constraintTop_toTopOf="@+id/outline" />

    <TextView
        android:id="@+id/tvOrdem"
        android:layout_width="169dp"
        android:layout_height="58dp"
        android:text="@string/tvordem"
        android:textAlignment="center"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/outline2"
        app:layout_constraintEnd_toEndOf="@+id/outline2"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="@+id/outline2"
        app:layout_constraintTop_toTopOf="@+id/outline2"
        app:layout_constraintVertical_bias="0.538" />

    <TextView
        android:id="@+id/carateristicas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginBottom="4dp"
        android:background="#FFFFFF"
        android:text="@string/caracter_sticas"
        android:textColor="#035815"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/tvQuestoes"
        app:layout_constraintEnd_toEndOf="@+id/outline"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="@+id/outline"
        app:layout_constraintTop_toBottomOf="@+id/outline2" />

    <TextView
        android:id="@+id/tvDescription"
        android:layout_width="280dp"
        android:layout_height="93dp"
        android:scrollbarThumbVertical="@android:color/holo_green_dark"
        android:scrollbars="vertical"
        android:text="@string/tvdescricao"
        android:textColor="#000000"
        android:verticalScrollbarPosition="right"
        app:layout_constraintBottom_toBottomOf="@+id/outline3"
        app:layout_constraintEnd_toEndOf="@+id/outline3"
        app:layout_constraintStart_toStartOf="@+id/outline3"
        app:layout_constraintTop_toTopOf="@+id/outline3"
        app:layout_constraintVertical_bias="0.533" />

    <TextView
        android:id="@+id/outlineMap"
        android:layout_width="300dp"
        android:layout_height="222dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/outline_green"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/outline3" />

    <fragment
            android:id="@+id/map"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="286dp"
            android:layout_height="205dp"
            app:layout_constraintBottom_toBottomOf="@+id/outlineMap"
            app:layout_constraintEnd_toEndOf="@+id/outlineMap"
            app:layout_constraintStart_toStartOf="@+id/outlineMap"
            app:layout_constraintTop_toTopOf="@+id/outlineMap"
            tools:context=".MapsActivity" />

    <TextView
        android:id="@+id/carateristicas2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:layout_marginBottom="2dp"
        android:background="#FFFFFF"
        android:text="@string/localiza_o"
        android:textColor="#035815"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/outlineMap"
        app:layout_constraintEnd_toEndOf="@+id/outlineMap"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="@+id/outlineMap"
        app:layout_constraintTop_toBottomOf="@+id/outline3"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnSave"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="#035815"
        android:drawableLeft="@drawable/ic_add"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:text="@string/guardar"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <Button
        android:id="@+id/btnCancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="52dp"
        android:backgroundTint="#7E1B1B"
        android:drawableLeft="@drawable/ic_cancel"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:text="@string/cancelar"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="8dp"
        android:elegantTextHeight="true"
        android:text="@string/info_marcador"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="12sp"
        app:drawableLeftCompat="@drawable/ic_info"
        app:drawableTint="#43A6C6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/outlineMap" />

</androidx.constraintlayout.widget.ConstraintLayout>

